{
  "name": "arjanwestdorp/exposable",
  "description": "A laravel package to expose protected model in a secure way.",
  "keywords": [
    "arjan",
    "westdorp",
    "laravel",
    "expose"
  ],
  "homepage": "https://github.com/arjanwestdorp/exposable",
  "license": "MIT",
  "authors": [
    {
      "name": "Arjan Westdorp",
      "email": "arjanwestdorp@gmail.com",
      "homepage": "https://www.trait-development.nl",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.0.13",
    "illuminate/support": "^5.4 || ^6.0",
    "league/uri": "^5.3",
    "nesbot/carbon": "^1.22 || ^2.0"
  },
  "require-dev": {
    "mockery/mockery": "^0.9.5",
    "laravel/framework": "^5.4.18",
    "orchestra/database": "~3.4.0",
    "orchestra/testbench": "~3.4.7",
    "phpunit/phpunit": "^6.5"
  },
  "autoload": {
    "psr-4": {
      "ArjanWestdorp\\Exposable\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ArjanWestdorp\\Exposable\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  }
}
